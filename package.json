{
  "name": "sistema-gestion-asistencia",
  "version": "1.0.0",
  "description": "Sistema de Gestión de Asistencia para envíaseo E.S.P.",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../backend-alcaldia && npm install",
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd frontend && npm start",
    "start-alcaldia": "cd backend-alcaldia && node server-new.js",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "dev-all": "concurrently \"npm run start-backend\" \"npm run start-frontend\" \"npm run start-alcaldia\"",
    "start-cmd": "powershell -ExecutionPolicy Bypass -File start-all-cmd.ps1",
    "start-single-cmd": "powershell -ExecutionPolicy Bypass -File start-all-single-cmd.ps1",
    "start-original": "powershell -ExecutionPolicy Bypass -File start-all.ps1",
    "dev-team": "powershell -ExecutionPolicy Bypass -File team-setup/scripts/dev-team.ps1",
    "setup-team": "powershell -ExecutionPolicy Bypass -File team-setup/scripts/setup-team.ps1",
    "verify-sync": "powershell -ExecutionPolicy Bypass -File team-setup/scripts/verify-and-sync.ps1",
    "check-project": "powershell -ExecutionPolicy Bypass -File team-setup/scripts/check-project.ps1",
    "start-simple": "powershell -ExecutionPolicy Bypass -File team-setup/scripts/start-project.ps1",
    "build": "cd frontend && npm run build",
    "test": "echo \"No tests specified\" && exit 0",
    "git-status": "git status",
    "git-log": "git log --oneline -10",
    "git-pull": "git pull origin main",
    "git-push": "git push origin main"
  },
  "keywords": [
    "asistencia",
    "horas-extras",
    "excel",
    "react",
    "nodejs"
  ],
  "author": "envíaseo E.S.P.",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "react-router-dom": "^7.7.0"
  }
}
